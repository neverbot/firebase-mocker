{
  "name": "firebase-mocker",
  "version": "0.0.1",
  "description": "A modern TypeScript-based Firestore emulator server for local development and testing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test npx c8 mocha",
    "test:only": "NODE_ENV=test npx mocha",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "lint:quiet": "npx eslint . --quiet"
  },
  "files": [
    "dist",
    "src",
    "readme.md",
    "local_install.md"
  ],
  "keywords": [
    "firebase",
    "firestore",
    "emulator",
    "mock",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.19.30",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "firebase": "^10.13.0",
    "firebase-admin": "^12.4.0",
    "globals": "^15.11.0",
    "mocha": "^11.7.5",
    "prettier": "^3.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.53.1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.7.13",
    "dotenv": "^16.4.5",
    "express": "^4.18.2"
  }
}
